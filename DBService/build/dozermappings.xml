<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping>
        <class-a>document.model.Person</class-a>
        <class-b>document.model.beans.PersonBean</class-b>
        <field>
            <a>firstname</a>
            <b>firstname</b>
        </field>
        <field>
            <a>idPerson</a>
            <b>id</b>
        </field>
        <field>
            <a>lastname</a>
            <b>lastname</b>
        </field>
        <field>
            <a>birthdate</a>
            <b>birthdate</b>
        </field>
    </mapping>
    <mapping>
        <class-a>document.model.Measure</class-a>
        <class-b>document.model.beans.MeasureBean</class-b>
        <field>
            <a>value</a>
            <b>measureValue</b>
        </field>
        <field>
            <a>created</a>
            <b>dateRegistered</b>
        </field>
        <field>
            <a>measureType</a>
            <b>measureType</b>
        </field>
    </mapping>
    <mapping>
        <class-a>document.model.MeasureType</class-a>
        <class-b>document.model.beans.MeasureTypeBean</class-b>
        <field>
            <a>measureType</a>
            <b>measureType</b>
        </field>
    </mapping>
</mappings>